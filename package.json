{
  "name": "react-nlp-annotate",
  "version": "0.1.21",
  "homepage": "https://workaroundonline.github.io/react-nlp-annotate/",
  "dependencies": {},
  "scripts": {
    "build": "npm run build:babel && cp ./package.json ./dist/package.json",
    "build:babel": "NODE_ENV=production babel ./src --out-dir=./dist",
    "release": "npm run build && cd dist && npm publish",
    "storybook": "start-storybook -p 9050 -s public",
    "demosite": "react-scripts start",
    "build-storybook": "build-storybook -s public",
    "build:gh-pages": "react-scripts build",
    "deploy:gh-pages": "gh-pages -d build",
    "gh-pages": "npm run build:gh-pages && npm run deploy:gh-pages"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@storybook/addon-actions": "^4",
    "@storybook/addon-links": "^4",
    "@storybook/addons": "^4",
    "@storybook/react": "^4",
    "axios": "^0.19.0",
    "chroma-js": "^2.0.3",
    "downloadjs": "^1.4.7",
    "flow-bin": "^0.92.1",
    "gh-pages": "^2.0.1",
    "js-base64": "^2.5.1",
    "lodash": "^4.17.11",
    "query-string": "^6.8.1",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-monaco-editor": "^0.26.2",
    "react-scripts": "2.1.3",
    "react-select": "^3.0.8",
    "react-syntax-highlighter": "^10.3.0",
    "spelling": "^2.0.1"
  }
}
